{
  "expo": {
    "name": "app-merkator-feiras",
    "slug": "app-merkator-feiras",
    "description": "Aplicativo Merkator Feiras",
    "owner": "ricardoaguiar",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "extra": {
      "oneSignalAppId": "695db98b-cc06-473a-b444-fff34caa6bc9"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.reactjs.native.example.AppRealmDb",
      "buildNumber": "15"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ricardoaguiar.appmerkatorexpo",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
